{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analysis": "cross-env VUE_APP_MODE=analysis vue-cli-service serve --port 8080",
    "evaluation": "cross-env VUE_APP_MODE=evaluation vue-cli-service serve --port 8081",
    "mixed": "cross-env VUE_APP_MODE=analysis vue-cli-service serve --port 8080",
    "build:analysis": "cross-env NODE_ENV=production vue-cli-service build --mode analysis",
    "build:evaluation": "cross-env NODE_ENV=production vue-cli-service build --mode evaluation"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "copy-anything": "^4.0.5",
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "less": "^4.4.0",
    "less-loader": "^12.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}